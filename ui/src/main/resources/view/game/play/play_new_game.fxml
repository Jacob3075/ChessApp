<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?><?import javafx.scene.text.Font?>
<HBox minHeight="600.0" minWidth="900.0" prefHeight="358.0" prefWidth="805.0"
    xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.jacob.ui.game.play.PlayNewGameController">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="600.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <fx:include source="../board.fxml" fx:id="board" />
        <GridPane GridPane.columnIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="203.0" minHeight="10.0" prefHeight="114.99999237060547"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="319.00000762939453" minHeight="10.0" prefHeight="301.00000762939453"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="184.0" minHeight="10.0" prefHeight="174.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <fx:include source="timer.fxml" fx:id="timer" />
            <AnchorPane prefHeight="300.0" prefWidth="260.0" GridPane.rowIndex="1">
                <GridPane.margin>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="5.0"/>
                </GridPane.margin>
                <TableView fx:id="displayMovesTable" layoutY="1.0" prefHeight="292.0" prefWidth="260.0"
                           style="-fx-background-color: F3E2C9;">
                    <columns>
                        <TableColumn prefWidth="56.33331298828125" text="Moves" />
                        <TableColumn prefWidth="100.33331298828125" text="White" />
                        <TableColumn prefWidth="103.0" text="Black" />
                    </columns>
                </TableView>
            </AnchorPane>
            <AnchorPane>
                <Label contentDisplay="CENTER" layoutX="65.0" layoutY="21.0" text="Chess"
                       textAlignment="CENTER">
                    <font>
                        <Font name="Arial Black" size="51.0"/>
                    </font>
                </Label>
            </AnchorPane>
        </GridPane>
    </GridPane>
</HBox>
